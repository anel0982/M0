                Title = "Thông Báo!!",
                Content = "Hiện Gui Tp",
                Buttons = {
                    {
                        Title = "Được",
                        Callback = function()
                            local Gui = Instance.new("ScreenGui")
local BackGround = Instance.new("Frame")
